Point	./src/point.rb	/^class Point$/;"	c
SVG	./src/svg.rb	/^class SVG$/;"	c
SVGPoint	./src/svg_point.rb	/^class SVGPoint$/;"	c
initialize	./src/point.rb	/^	def initialize(x, y)$/;"	f	class:Point
initialize	./src/svg.rb	/^	def initialize(source) $/;"	f	class:SVG
initialize	./src/svg_point.rb	/^	def initialize(type, p = Point.new(nil, nil), r1 = Point.new(nil, nil), r2 = Point.new(nil, nil), keep = false)$/;"	f	class:SVGPoint
to_s	./src/point.rb	/^	def to_s$/;"	f	class:Point
